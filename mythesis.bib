@article{Limits_on_neutrino_oscillations_in_the_appearance_channels_numu_to_nue_and_numubar_to_nuebar,
title = {Limits on neutrino oscillations in the appearance channels $\numu \longrightarrow \nue and \numubar \longrightarrow \nuebar$},
journal = {Nuclear Physics B - Proceedings Supplements},
volume = {38},
number = {1},
pages = {235-239},
year = {1995},
note = {Neutrino 94},
issn = {0920-5632},
doi = {https://doi.org/10.1016/0920-5632(94)00752-H},
url = {https://www.sciencedirect.com/science/article/pii/092056329400752H},
author = {B. Armbruster and B.E. Bodmann and I. Blair and N.E. Booth and A.C. Dodd and G. Drexlin and V. Eberhard and J.A. Edgington and K. Eitel and M. Ferstl and E. Finckh and H. Gemmeke and W. Grandegger and J. Höβ and T. Jannakos and M. Kleifges and J. Kleinfeller and W. Kretschmer and R. Maschuw and J. Rapp and P. Plischke and F. Schilling and B. Seligmann and O. Stumm and J. Wolf and S. Wölfle and B. Zeitnitz},
abstract = {The KARMEN experiment at the pulsed spallation neutron facility ISIS uses the beam stop neutrinos \numu, \nue and \numubar from $\pi^+$ and $\mu^+$ decay at rest to search for neutrino oscillations in the appearance modes \numu \longrightarrow \nue and \numubar \longrightarrow \nuebar. A high resolution 56 ton liquid scintillation calorimeter located at a mean distance of 17.5 m from the proton beam stop allows identification of \nue and \nuebar with spectroscopic quality. We report the status of our search for neutrino oscillations after four years of running. No positive evidence for neutrino oscillations has been observed in both appearance channels. The 90 \% CL limits of this experiment exclude mixing angles $\sin^2{2\theta} \geq 0.0062$ in the $\numubar \longrightarrow \nuebar$ channel and $\sin^2{2\theta} \geq 0.048$ in the $\numu \longrightarrow \nue$ channel for the region $\Deltam^2 > 1$ eV$^2$}
}

